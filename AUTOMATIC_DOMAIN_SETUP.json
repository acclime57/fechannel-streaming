{
  "project": {
    "name": "fechannel",
    "description": "FEChannel.com Professional Streaming Platform",
    "type": "full-stack-application"
  },
  "domains": {
    "primary": "fechannel.com",
    "aliases": ["www.fechannel.com"],
    "ssl": true,
    "redirect_www": true
  },
  "deployment": {
    "source_url": "https://tid29gwi5y.space.minimax.io",
    "status": "ready",
    "features": [
      "netflix-style-frontend",
      "professional-admin-dashboard", 
      "aws-s3-integration",
      "roku-direct-publisher",
      "analytics-system",
      "video-management"
    ]
  },
  "routing": {
    "/": "Netflix-style streaming homepage",
    "/admin": "Professional admin dashboard",
    "/dashboard": "Analytics and reporting",
