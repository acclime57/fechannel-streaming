  "verification": {
    "endpoints": [
      "https://fechannel.com",
      "https://fechannel.com/admin",
      "https://fechannel.com/api/test"
    ],
    "expected_status": 200
  },
  "automation": {
    "deployment_script": "deploy-to-fechannel.sh",
    "vercel_config": "vercel.json",
    "package_config": "package.json"
  }
}